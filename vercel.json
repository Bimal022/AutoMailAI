{
    "version": 2,
    "builds": [
      {
        "src": "app.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/",
        "status": 301,
        "headers": {
          "Location": "/auth"
        }
      },
      {
        "src": "/(.*)",
        "dest": "/app.js"
      }
    ]
  }
  